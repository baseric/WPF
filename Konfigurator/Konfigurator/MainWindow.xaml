<Window x:Class="Konfigurator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Konfigurator" Height="460" Width="965">
    <Grid Width="954">
        <Grid.RowDefinitions>
            <RowDefinition Height="49*"></RowDefinition>
            <RowDefinition Height="331*"></RowDefinition>
            <RowDefinition Height="41*"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="172*"></ColumnDefinition>
            <ColumnDefinition Width="711*"></ColumnDefinition>
            <ColumnDefinition Width="71*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Label Content="Witaj w naszym konfiguratorze" Grid.Column="1" Height="28" Margin="244,12,245,0" Name="label1" VerticalAlignment="Top" />
        <Button Content="dalej" Grid.Column="1" Grid.Row="2" Height="23" Margin="0,12,114,0" Name="button1" VerticalAlignment="Top" Click="button1_Click" HorizontalAlignment="Right" Width="75" />
        <Button Content="zamknij" Grid.Column="1" Grid.Row="2" Height="23" HorizontalAlignment="Right" Margin="0,12,19,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" />
        <DockPanel Grid.Row="1" Height="131" HorizontalAlignment="Left" Name="dockPanel1" VerticalAlignment="Top" Width="140" ></DockPanel>
    </Grid>
    
</Window>
